    },
    "operation_cleared_(3-star)": {
      "id": "Operation_cleared_(3-star)",
      "text": "That was some beautiful teamwork. Great work, everyone.",
      "links": [],
      "audio": {
        "JP": "Ch%27en_the_Holungday-028.ogg",
        "CN": "Ch%27en_the_Holungday-028-CN.ogg",
