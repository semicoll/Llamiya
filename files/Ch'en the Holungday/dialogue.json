    "operation_cleared_(4-star)": {
      "id": "Operation_cleared_(4-star)",
      "text": "Pure evil and a little torment is nothing to be afraid of. But there's never anything pure there, that's what's terrifying.",
      "links": [],
      "audio": {
        "JP": "Ch%27en_the_Holungday-027.ogg",
        "CN": "Ch%27en_the_Holungday-027-CN.ogg",
        "EN": "Ch%27en_the_Holungday-027-EN.ogg",
