    },
    "skill_activation_4": {
      "id": "Skill_activation_#4",
      "text": "Do not disrupt my thought process.",
      "links": [],
      "audio": {
        "JP": "Gnosis-026.ogg",
        "CN": "Gnosis-026-CN.ogg",
        "EN": "Gnosis-026-EN.ogg",
