        "JP": "Gnosis-020.ogg",
        "CN": "Gnosis-020-CN.ogg",
        "EN": "Gnosis-020-EN.ogg",
        "KR": "Gnosis-020-KR.ogg"
      }
    },
    "deployed_1": {
      "id": "Deployed_#1",
      "text": "A fine choice.",
      "links": [],
      "audio": {
        "JP": "Gnosis-021.ogg",
        "CN": "Gnosis-021-CN.ogg",
        "EN": "Gnosis-021-EN.ogg",
        "KR": "Gnosis-021-KR.ogg"
