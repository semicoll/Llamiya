    "selected_2": {
      "id": "Selected_#2",
      "text": "Loud and clear!",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Ceobe-020.ogg",
        "CN": "Ceobe-020-CN.ogg",
        "EN": "Ceobe-020-EN.ogg",
        "KR": "Ceobe-020-KR.ogg"
      }
    },
    "deployed_1": {
      "id": "Deployed_#1",
      "text": "Leave it to me!",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Ceobe-021.ogg",
        "CN": "Ceobe-021-CN.ogg",
        "EN": "Ceobe-021-EN.ogg",
        "KR": "Ceobe-021-KR.ogg"
      }
    },
    "deployed_2": {
      "id": "Deployed_#2",
      "text": "See you later!",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Ceobe-022.ogg",
        "CN": "Ceobe-022-CN.ogg",
        "EN": "Ceobe-022-EN.ogg",
        "KR": "Ceobe-022-KR.ogg"
      }
    },
