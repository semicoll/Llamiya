    "operation_cleared_(4-star)": {
      "id": "Operation_cleared_(4-star)",
      "text": "With your brains, my job is super easy! You're the best, Doctor!",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Ceobe-027.ogg",
        "CN": "Ceobe-027-CN.ogg",
        "EN": "Ceobe-027-EN.ogg",
        "KR": "Ceobe-027-KR.ogg"
      }
    },
    "operation_cleared_(3-star)": {
      "id": "Operation_cleared_(3-star)",
      "text": "All that's needed is a bang, bang! And that takes care of them!",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Ceobe-028.ogg",
        "CN": "Ceobe-028-CN.ogg",
        "EN": "Ceobe-028-EN.ogg",
        "KR": "Ceobe-028-KR.ogg"
      }
    },
    "operation_cleared_(2-star)": {
