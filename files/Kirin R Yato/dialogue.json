      }
    },
    "operation_failed": {
      "id": "Operation_failed",
      "text": "This falls on me. You make a retreat, and grant me one more attempt.",
      "links": [],
      "audio": {
        "JP": "Kirin_R_Yato-030.ogg",
        "CN": "Kirin_R_Yato-030-CN.ogg",
        "EN": "Kirin_R_Yato-030-EN.ogg",
