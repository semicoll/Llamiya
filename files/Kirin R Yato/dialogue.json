    "tap": {
      "id": "Tap",
      "text": "What is it, Doctor? Intrigued by the materials for this outfit?",
      "links": [],
      "audio": {
        "JP": "Kirin_R_Yato-032.ogg",
        "CN": "Kirin_R_Yato-032-CN.ogg",
        "EN": "Kirin_R_Yato-032-EN.ogg",
        "KR": "Kirin_R_Yato-032-KR.ogg"
      }
    },
    "trust_tap": {
      "id": "Trust_tap",
      "text": "Oh, hey, Doctor. I guess I'm not used to dropping the mask after all.",
      "links": [],
      "audio": {
        "JP": "Kirin_R_Yato-033.ogg",
        "CN": "Kirin_R_Yato-033-CN.ogg",
        "EN": "Kirin_R_Yato-033-EN.ogg",
        "KR": "Kirin_R_Yato-033-KR.ogg"
      }
    },
    "greetings": {
      "id": "Greetings",
      "text": "Hey, Doctor. You're the same as ever. I wonder if I'm any better than I used to be?",
