    "assigned_to_[[riic]]": {
      "id": "Assigned_to_[[RIIC]]",
      "text": "I'd like more dangerous or challenging work, but if these are your orders, I'll gladly accept, Doctor.",
      "links": [],
      "audio": {
        "JP": "Kirin_R_Yato-031.ogg",
        "CN": "Kirin_R_Yato-031-CN.ogg",
        "EN": "Kirin_R_Yato-031-EN.ogg",
