    "operation_cleared_(4-star)": {
      "id": "Operation_cleared_(4-star)",
      "text": "We owe this victory to your fine command. I still have a long way to go.",
      "links": [],
      "audio": {
        "JP": "Kirin_R_Yato-027.ogg",
        "CN": "Kirin_R_Yato-027-CN.ogg",
        "EN": "Kirin_R_Yato-027-EN.ogg",
        "KR": "Kirin_R_Yato-027-KR.ogg"
      }
    },
    "operation_cleared_(3-star)": {
      "id": "Operation_cleared_(3-star)",
      "text": "Take victory as a whetstone to hone the cutting edge.",
      "links": [],
      "audio": {
        "JP": "Kirin_R_Yato-028.ogg",
        "CN": "Kirin_R_Yato-028-CN.ogg",
        "EN": "Kirin_R_Yato-028-EN.ogg",
        "KR": "Kirin_R_Yato-028-KR.ogg"
      }
    },
    "operation_cleared_(2-star)": {
      "id": "Operation_cleared_(2-star)",
      "text": "Was this your mercy? Understood.",
      "links": [],
