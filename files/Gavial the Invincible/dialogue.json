      }
    },
    "assigned_to_[[riic]]": {
      "id": "Assigned_to_[[RIIC]]",
      "text": "I guess I'll drop in at Medical before the mission.",
      "links": [],
      "audio": {
        "JP": "Gavial_the_Invincible-031.ogg",
        "CN": "Gavial_the_Invincible-031-CN.ogg",
        "EN": "Gavial_the_Invincible-031-EN.ogg",
        "KR": "Gavial_the_Invincible-031-KR.ogg"
      }
    },
    "tap": {
      "id": "Tap",
      "text": "What's the rush, Doc? Not like I'm gonna run away.",
      "links": [],
      "audio": {
        "JP": "Gavial_the_Invincible-032.ogg",
        "CN": "Gavial_the_Invincible-032-CN.ogg",
        "EN": "Gavial_the_Invincible-032-EN.ogg",
        "KR": "Gavial_the_Invincible-032-KR.ogg"
      }
    },
    "trust_tap": {
      "id": "Trust_tap",
      "text": "You look pretty free to me, Doctor. Just so happens I am too. Wanna take a walk?",
      "links": [],
      "audio": {
        "JP": "Gavial_the_Invincible-033.ogg",
        "CN": "Gavial_the_Invincible-033-CN.ogg",
        "EN": "Gavial_the_Invincible-033-EN.ogg",
        "KR": "Gavial_the_Invincible-033-KR.ogg"
      }
    },
    "greetings": {
