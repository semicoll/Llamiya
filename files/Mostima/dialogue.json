    "[[skill]]_activation_1": {
      "id": "[[Skill]]_activation_#1",
      "text": "Seventy percent-ish should do the trick.",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Mostima-023.ogg",
        "CN": "Mostima-023-CN.ogg",
        "EN": "Mostima-023-EN.ogg",
        "KR": "Mostima-023-KR.ogg"
      }
    },
    "skill_activation_2": {
      "id": "Skill_activation_#2",
      "text": "Isn't it about time to wrap things up?",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Mostima-024.ogg",
        "CN": "Mostima-024-CN.ogg",
        "EN": "Mostima-024-EN.ogg",
        "KR": "Mostima-024-KR.ogg"
      }
    },
    "skill_activation_3": {
      "id": "Skill_activation_#3",
      "text": "I have nothing against you guys, but this is just a part of life.",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Mostima-025.ogg",
        "CN": "Mostima-025-CN.ogg",
        "EN": "Mostima-025-EN.ogg",
        "KR": "Mostima-025-KR.ogg"
      }
    },
    "skill_activation_4": {
      "id": "Skill_activation_#4",
      "text": "What should we get to eat after this?",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Mostima-026.ogg",
        "CN": "Mostima-026-CN.ogg",
        "EN": "Mostima-026-EN.ogg",
        "KR": "Mostima-026-KR.ogg"
      }
    },
    "operation_cleared_(4-star)": {
      "id": "Operation_cleared_(4-star)",
      "text": "Some thrill-seeking every now and then isn't half bad either.",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Mostima-027.ogg",
        "CN": "Mostima-027-CN.ogg",
        "EN": "Mostima-027-EN.ogg",
        "KR": "Mostima-027-KR.ogg"
      }
    },
    "operation_cleared_(3-star)": {
      "id": "Operation_cleared_(3-star)",
      "text": "Hm? I thought I missed a few, but I guess that's a job well done.",
      "links": [],
      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Mostima-028.ogg",
        "CN": "Mostima-028-CN.ogg",
        "EN": "Mostima-028-EN.ogg",
        "KR": "Mostima-028-KR.ogg"
      }
    },
    "operation_cleared_(2-star)": {
      "id": "Operation_cleared_(2-star)",
      "text": "For a transporter, the results of a battle don't really matter. Just surviving is enough of a victory.",
      "links": [],
