      "audio": {
        "JP": "https://arknights.fandom.com/wiki/File:Ch%27en-014.ogg",
        "CN": "Ch%27en-014-CN.ogg",
        "EN": "Ch%27en-014-EN.ogg",
        "KR": "Ch%27en-014-KR.ogg"
      }
    },
    "added_to_squad": {
